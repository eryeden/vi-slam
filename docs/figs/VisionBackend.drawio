        <mxCell id="y6ONzhfBYeOvc6R4NJ2u-26" value="\( P(Z_1 | X_1) \)" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" parent="malhQMyiqFlj0tt06wjK-1" vertex="1">
