        <mxCell id="iuoN6O5FOuv3n4ogCXSz-8" value="ｑ" style="ellipse;whiteSpace=wrap;html=1;aspect=fixed;align=center;" vertex="1" parent="malhQMyiqFlj0tt06wjK-1">
