  <diagram id="CG2pjZPTrM2GcYqYaMGn" name="GTSAM">
    <mxGraphModel dx="1317" dy="894" grid="1" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1" fold="1" page="1" pageScale="1" pageWidth="827" pageHeight="1169" math="1" shadow="0">
