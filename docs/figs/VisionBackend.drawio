        </mxCell>
        
        <mxCell id="y6ONzhfBYeOvc6R4NJ2u-24" value="Xが決まると&lt;br&gt;この平面で凸をスライスできる" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" parent="malhQMyiqFlj0tt06wjK-1" vertex="1">
          
